

<% #binding.pry %>
<div class="con_width">
  <div class="rw_list">
    <div class="rl_left">
      <%= render 'spree/users/sidebar' %>
      <!-- end .rl_left -->
    </div>
    <div class="rl_right">
      <div class="mos_title"><h1>Edit Address</h1></div>
      <div class="ast_con">
      <div data-hook="account_my_orders" class="account-my-orders">

      <div class="dsc_add1">
        <div class="dsa_title">
          <div class="dat_left"><%= @address.state_name %></div>
          <div class="dat_right">
                  <% if @address.is_current %>
                   <span class="drt_current">DEFAULT ADDRESS</span>
                  <% end %>
                  <span><%= link_to "Delete", account_address_path(@address.to_param), :method => :delete %> &nbsp; |</span>
          </div>
        </div>
        
        <%= render 'form', :add => @address %>

      </div>

			<% @addresses.each do |add| %>
        
          <div class="dsx_con2">

      		<div class="dsc_add1">
      			<div class="dsa_title">
      				<div class="dat_left"><%= add.state_name %></div>
      				<div class="dat_right">
                <% if add.is_current %>
                 <span class="drt_current">DEFAULT ADDRESS</span>
                <% end %>
      					<span><%= link_to "Delete", account_address_path(add.to_param), :method => :delete %> &nbsp;|</span>
      					<span><%= link_to "Edit", edit_account_address_path(add) %> &nbsp;|</span>
      				</div>
      			</div>
            <%= render 'address_list', :add => add %>
      			
      		</div>
          <!-- end dsx_con2 -->
          </div>  
      	<% end %>	
      	
        <%#= render 'spree/users/order_list' %>        
        <br />
      </div>
         <!-- end ast_con -->
       </div>
      <div class="rlt_bottom"> </div>
      <!-- end .rl_right -->
    </div>
    <!-- end .rw_list -->
  </div>
  <!-- end .con_width -->
</div>





